    --labels \\"vm:\${AWS_INSTANCE_TYPE}\\" \\\\
    --work \\"\${RUNNER_WORKDIR}\\" \\\\
    --runnergroup \\"\${RUNNER_GROUP}\\" \\\\
    --unattended \\\\
    --ephemeral
}

run() {
  trap deregister_runner SIGINT SIGQUIT SIGTERM INT QUIT TERM
  sudo -u ubuntu timeout --signal=15 \\"\${RUNNER_TIMEOUT}\\" ./run.sh & wait $! && return
  deregister_runner
}

