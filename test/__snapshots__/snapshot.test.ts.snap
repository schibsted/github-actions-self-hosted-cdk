    -H \\"Accept: application/vnd.github.v3+json\\" \\\\
    -H \\"\${AUTH_HEADER}\\" \\\\
    \\"\${TOKEN_REGISTRATION_URL}\\" \\\\
    | jq -r '.token')\\"
}

configure() {
